                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding ProjectileDamage}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Amplitude Exponent:" DataContext="{Binding ProjectileDamage}"
                                        Value="{Binding AmplitudeExponent, Mode=TwoWay}"
                                        Units=""
                                        Minimum="0.01" Maximum="1" Step="0.01"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Length Exponent:" DataContext="{Binding ProjectileDamage}"
                                        Value="{Binding LengthExponent, Mode=TwoWay}"
                                        Units=""
                                        Minimum="0.01" Maximum="1" Step="0.01"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Missile Damage -->
                <controls:VolumeControl Header="Missile Damage" DataContext="{Binding MissileDamage}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding MissileDamage}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Max Length:" DataContext="{Binding MissileDamage}"
                                        Value="{Binding MaxLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="100" Maximum="1500" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding MissileDamage}"
                                        Value="{Binding AttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding MissileDamage}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Amplitude Exponent:" DataContext="{Binding MissileDamage}"
                                        Value="{Binding AmplitudeExponent, Mode=TwoWay}"
                                        Units=""
                                        Minimum="0.01" Maximum="1" Step="0.01"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Length Exponent:" DataContext="{Binding MissileDamage}"
                                        Value="{Binding LengthExponent, Mode=TwoWay}"
                                        Units=""
                                        Minimum="0.01" Maximum="1" Step="0.01"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Melee Damage -->
                <controls:VolumeControl Header="Melee Damage" DataContext="{Binding MeleeDamage}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
