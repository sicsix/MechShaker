                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Max Length:" DataContext="{Binding ProjectileDamage}"
                                        Value="{Binding MaxLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="100" Maximum="1500" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding ProjectileDamage}"
