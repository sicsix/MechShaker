<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:MechShakerUI.ViewModels"
             xmlns:controls="clr-namespace:MechShakerUI.Views.Controls"
             mc:Ignorable="d" d:DesignWidth="1280" d:DesignHeight="680"
             x:Class="MechShakerUI.Views.SettingsView"
             x:DataType="viewModels:SettingsViewModel"
             x:CompileBindings="True"
             x:Name="ThisControl">

    <Grid RowDefinitions="*,Auto" ColumnDefinitions="*,Auto,*">
        <ScrollViewer Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalScrollBarVisibility="Visible">
            <StackPanel HorizontalAlignment="Center" Spacing="-20" Width="800">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" Width="612" Spacing="8"
                            VerticalAlignment="Center" Margin="0">
                    <TextBlock Text="Output Device:" Width="142" FontWeight="SemiBold" />
                    <ComboBox ItemsSource="{Binding Devices}"
                              SelectedItem="{Binding OutputDevice, Mode=TwoWay}"
                              Margin="70,0,0,0" Width="366" />
                </StackPanel>

                <!-- Master -->
                <controls:VolumeControl Header="Master" DataContext="{Binding Master, Mode=TwoWay}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />

                <!-- BEGIN Weapon Effects Group -->
                <!-- Lasers -->
                <controls:VolumeControl Header="Lasers" DataContext="{Binding Lasers}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding Lasers}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Frequency:" DataContext="{Binding Lasers}"
                                        Value="{Binding SecondaryFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Amplitude:" DataContext="{Binding Lasers}"
                                        Value="{Binding SecondaryAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- TAG -->
                <controls:VolumeControl Header="TAG" DataContext="{Binding TAG}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding TAG}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Frequency:" DataContext="{Binding TAG}"
                                        Value="{Binding SecondaryFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Amplitude:" DataContext="{Binding TAG}"
                                        Value="{Binding SecondaryAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Flamers -->
                <controls:VolumeControl Header="Flamers" DataContext="{Binding Flamers}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding Flamers}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Frequency:" DataContext="{Binding Flamers}"
                                        Value="{Binding SecondaryFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Amplitude:" DataContext="{Binding Flamers}"
                                        Value="{Binding SecondaryAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tert Frequency:" DataContext="{Binding Flamers}"
                                        Value="{Binding TertiaryFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Tert Amplitude:" DataContext="{Binding Flamers}"
                                        Value="{Binding TertiaryAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- MachineGuns -->
                <controls:VolumeControl Header="Machine Guns" DataContext="{Binding MachineGuns}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding MachineGuns}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Length:" DataContext="{Binding MachineGuns}"
                                        Value="{Binding Length, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="30" Maximum="400" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding MachineGuns}"
                                        Value="{Binding AttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding MachineGuns}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- AMS -->
                <controls:VolumeControl Header="AMS" DataContext="{Binding AMS}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding AMS}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Length:" DataContext="{Binding AMS}"
                                        Value="{Binding Length, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="30" Maximum="400" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding AMS}"
                                        Value="{Binding AttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding AMS}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Autocannons & Rifles  -->
                <controls:VolumeControl Header="ACs &amp; Rifles" DataContext="{Binding AutocannonsAndRifles}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Min Frequency:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding MinFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Max Frequency:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding MaxFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Min Amplitude:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding MinAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="10" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Min Length:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding MinLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="60" Maximum="2000" Step="20"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Max Length:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding MaxLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="60" Maximum="2000" Step="20"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding AttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding AutocannonsAndRifles}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- PPCs  -->
                <controls:VolumeControl Header="PPCs" DataContext="{Binding PPCs}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Pop Frequency:" DataContext="{Binding PPCs}"
                                        Value="{Binding PopFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Pop Length:" DataContext="{Binding PPCs}"
                                        Value="{Binding PopLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="60" Maximum="2000" Step="20"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Pop Attack End:" DataContext="{Binding PPCs}"
                                        Value="{Binding PopAttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding PopDecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Pop Decay Start:" DataContext="{Binding PPCs}"
                                        Value="{Binding PopDecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding PopAttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Frequency:" DataContext="{Binding PPCs}"
                                        Value="{Binding TailFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Tail Amplitude:" DataContext="{Binding PPCs}"
                                        Value="{Binding TailAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Length:" DataContext="{Binding PPCs}"
                                        Value="{Binding TailLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="60" Maximum="2000" Step="20"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Attack End:" DataContext="{Binding PPCs}"
                                        Value="{Binding TailAttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding TailDecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Decay Start:" DataContext="{Binding PPCs}"
                                        Value="{Binding TailDecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding TailAttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Missiles -->
                <controls:VolumeControl Header="Missiles" DataContext="{Binding Missiles}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Launch Frequency:" DataContext="{Binding Missiles}"
                                        Value="{Binding LaunchFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Launch Length:" DataContext="{Binding Missiles}"
                                        Value="{Binding LaunchLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="60" Maximum="2000" Step="20"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Launch Attack End:" DataContext="{Binding Missiles}"
                                        Value="{Binding LaunchAttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding LaunchDecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Launch Decay Start:" DataContext="{Binding Missiles}"
                                        Value="{Binding LaunchDecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding LaunchAttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Frequency:" DataContext="{Binding Missiles}"
                                        Value="{Binding TailFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Tail Amplitude:" DataContext="{Binding Missiles}"
                                        Value="{Binding TailAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Length:" DataContext="{Binding Missiles}"
                                        Value="{Binding TailLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="60" Maximum="2000" Step="20"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Attack End:" DataContext="{Binding Missiles}"
                                        Value="{Binding TailAttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding TailDecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Tail Decay Start:" DataContext="{Binding Missiles}"
                                        Value="{Binding TailDecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding TailAttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Stream Factor:" DataContext="{Binding Missiles}"
                                        Value="{Binding StreamFactor, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="5" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <!-- END Weapons Effects Group -->

                <!-- BEGIN Impact Effects Group -->
                <!-- Melee -->
                <controls:VolumeControl Header="Melee" DataContext="{Binding Melee}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Hit Frequency:" DataContext="{Binding Melee}"
                                        Value="{Binding HitFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Hit Length:" DataContext="{Binding Melee}"
                                        Value="{Binding HitLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="200" Maximum="800" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Hit Attack End:" DataContext="{Binding Melee}"
                                        Value="{Binding HitAttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding HitDecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Hit Decay Start:" DataContext="{Binding Melee}"
                                        Value="{Binding HitDecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding HitAttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Swing Frequency:" DataContext="{Binding Melee}"
                                        Value="{Binding SwingFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Swing Length:" DataContext="{Binding Melee}"
                                        Value="{Binding SwingLength, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="400" Maximum="1500" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Swing Attack End:" DataContext="{Binding Melee}"
                                        Value="{Binding SwingAttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding SwingDecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Swing Decay Start:" DataContext="{Binding Melee}"
                                        Value="{Binding SwingDecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding SwingAttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Swing Amplitude:" DataContext="{Binding Melee}"
                                        Value="{Binding SwingAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Mass Factor:" DataContext="{Binding Melee}"
                                        Value="{Binding MassFactor, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Footsteps -->
                <controls:VolumeControl Header="Footsteps" DataContext="{Binding Footsteps}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding Footsteps}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Length:" DataContext="{Binding Footsteps}"
                                        Value="{Binding Length, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="200" Maximum="800" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding Footsteps}"
                                        Value="{Binding AttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding Footsteps}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Speed Factor:" DataContext="{Binding Footsteps}"
                                        Value="{Binding SpeedFactor, Mode=TwoWay}" Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Mass Factor:" DataContext="{Binding Footsteps}"
                                        Value="{Binding MassFactor, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Landing Impacts -->
                <controls:VolumeControl Header="Landing Impacts" DataContext="{Binding LandingImpacts}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding LandingImpacts}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Length:" DataContext="{Binding LandingImpacts}"
                                        Value="{Binding Length, Mode=TwoWay}"
                                        Units="ms"
                                        Minimum="200" Maximum="1500" Step="10"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Attack End:" DataContext="{Binding LandingImpacts}"
                                        Value="{Binding AttackEnd, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="{Binding DecayStart}" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <controls:SliderControl Label="Decay Start:" DataContext="{Binding LandingImpacts}"
                                        Value="{Binding DecayStart, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="{Binding AttackEnd}" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />
                <!-- END Impact Effects Group -->


                <!-- BEGIN Damage Effects Group -->
                <!-- Laser Damage -->
                <controls:VolumeControl Header="Laser Damage" DataContext="{Binding LaserDamage}"
                                        Volume="{Binding Volume, Mode=TwoWay}"
                                        Enabled="{Binding Enabled, Mode=TwoWay}" />
                <controls:SliderControl Label="Frequency:" DataContext="{Binding LaserDamage}"
                                        Value="{Binding Frequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Frequency:" DataContext="{Binding LaserDamage}"
                                        Value="{Binding SecondaryFrequency, Mode=TwoWay}"
                                        Units="Hz"
                                        Minimum="10" Maximum="120" Step="1" />
                <controls:SliderControl Label="Sec Amplitude:" DataContext="{Binding LaserDamage}"
                                        Value="{Binding SecondaryAmplitude, Mode=TwoWay}"
                                        Units="%"
                                        Minimum="0" Maximum="100" Step="5"
                                        IsVisible="{Binding ((viewModels:SettingsViewModel)DataContext).AdvancedMode, ElementName=ThisControl}" />

                <!-- Projectile Damage -->
                <controls:VolumeControl Header="Projectile Damage" DataContext="{Binding ProjectileDamage}"
